{
    "name": "popphp/pop-color",
    "description": "Pop Color Component for Pop PHP Framework",
    "keywords": [
        "color converstion",
        "color values",
        "color management",
        "rgb",
        "cmyk",
        "gray",
        "grayscale",
        "hex",
        "hsl",
        "lab"
    ],
    "homepage": "https://github.com/popphp/pop-color",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nick Sagona",
            "email": "dev@noladev.com",
            "homepage": "https://www.popphp.org/"
        }
    ],
    "require": {
        "php": ">=8.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.0"
    },
    "autoload": {
        "psr-4": {
            "Pop\\Color\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pop\\Color\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
